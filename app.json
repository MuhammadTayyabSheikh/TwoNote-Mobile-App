{
	"expo": {
		"name": "TwoNote",
		"slug": "TwoNote1",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/splash.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./src/assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#002b36"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.twonote.www"
		},
		"web": {
			"favicon": "./src/assets/TwoNote-1.png"
		},
		"description": "One Safe Place For All Your Notes.",
		"notification": {
			"iosDisplayInForeground": true
		}
	}
}
